{
  "name": "oni-plugin-markdown-preview",
  "version": "0.0.1",
  "main": "lib/index.js",
  "engines": {
    "oni": "^0.0.1"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "test": "tsc -p tsconfig.test.json && mocha --recursive ./lib_test/test"
  },
  "oni": {
    "supportedFileTypes": [
      "markdown"
    ],
    "commands": {
      "markdown.preview": {
        "name": "Markdown Preview",
        "details": "Preview open markdown file"
      }
    }
  },
  "tbd-dependencies": {
    "marked": "^0.3.6",
    "dompurify": "1.0.2",
    "oni-types": "^0.0.4",
    "oni-api": "^0.0.9"
  },
  "tbd-peerDependencies": {
    "typescript": "^2.5.3",
    "rimraf": "^2.6.2"
  },
  "tbd-devDependencies": {
    "typescript": "2.5.3",
    "rimraf": "2.6.2"
  }
}
